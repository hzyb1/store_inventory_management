<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org/DTD Mapper 3.0" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="mapper.StockMapper">

  <select id="selectById" parameterType="int" resultType="model.Stock">

    select * from stock where id=#{id}

  </select>
  <select  id="selectAll" resultType="model.Stock">
  
    select* from stock
  </select>
  <update id="updateStock" parameterType="model.Stock">
      update stock set account=#{account},productId=#{productId},areanumber=#{areanumber},
		shelfnumber=#{shelfnumber},instocktime=#{instocktime}
		where id = #{id}
  
  </update>
  <insert id="insertStock"  useGeneratedKeys="true" keyProperty="id">
		insert into stock(account,productId,areanumber,shelfnumber,instocktime)
			 values(#{account},#{productId},#{areanumber},#{shelfnumber},#{instocktime})
  
  </insert>
  <delete  id ="deleteById" parameterType="int">
		delete from stock where id=#{id}
   </delete>
</mapper>
